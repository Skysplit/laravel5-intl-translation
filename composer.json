{
    "name": "skysplit/laravel5-intl-translation",
    "type": "library",
    "license": "MIT",
    "description": "Laravel 5 package for better translation syntax using php-intl extension",
    "keywords": [
        "laravel",
        "translation",
        "validation",
        "intl"
    ],
    "authors": [
        {
            "name": "Tomasz Romik",
            "email": "manetamajster@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Skysplit\\Laravel\\Translation\\": "src/"
        }
    },
    "autoload-dev": {
        "classmap": [
            "tests/"
        ]
    },
    "require": {
        "ext-intl": "*",
        "illuminate/support": "^5.0",
        "illuminate/translation": "^5.0",
        "illuminate/validation": "^5.0"
    },
    "require-dev": {
        "orchestra/testbench": "^3.2",
        "phpunit/phpunit": "^4.0|^5.0"
    }
}
